{
    "ace": {
        "@behavioursEnabled": true,
        "@fontSize": 12,
        "@keyboardmode": "default",
        "@showFoldWidgets": false,
        "@showGutter": true,
        "@showInvisibles": false,
        "@showLineNumbers": true,
        "@showPrintMargin": false,
        "@theme": "ace/theme/cloud9_night",
        "custom-types": {
            "json()": {
                "settings": "javascript"
            }
        },
        "statusbar": {
            "@show": "true"
        }
    },
    "breakpoints": {
        "@active": true
    },
    "collab": {
        "@show-author-info": true,
        "@timeslider-visible": false
    },
    "cs50": {
        "presentation": {
            "@editorFontSize": 24,
            "@presenting": false,
            "@terminalFontSize": 24
        }
    },
    "debug": {
        "@pause": 0
    },
    "general": {
        "@skin": "flat-dark"
    },
    "language": {
        "@continuousCompletion": false,
        "@enterCompletion": false
    },
    "openfiles": {
        "@show": true
    },
    "output": {},
    "projecttree": {
        "@showhidden": true
    },
    "tabs": {
        "@asterisk": false,
        "@show": true,
        "editorTypes": {
            "@class": "none",
            "@exe": "none",
            "@gz": "none",
            "@o": "none",
            "@pyc": "none",
            "@raw": "none",
            "@tar": "none",
            "@zip": "none"
        }
    },
    "terminal": {
        "@fontsize": 12,
        "@scrollback": 2000
    }
}